{
  "name": "googledocs-clone",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0 < 17.0.0",
    "yarn": ">=1.21.0 <2.0.0"
  },
  "scripts": {
    "dev": "concurrently \"yarn workspace server dev\" \"yarn workspace client dev\"",
    "production": "concurrently \"yarn workspace client build\" \"yarn workspace server production\" ",
    "pm": "concurrently \"pm2 start ecosystem.config.js --attach\"",
    "pm-dev": "concurrently \"yarn workspace server pm-dev\"",
    "del": "concurrently \"pm2 delete all\"",
    "client-build": "concurrently \"yarn workspace client build\""
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@fastify/session": "^8.1.0",
    "connect-redis": "^6.1.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify": "^3.28.0",
    "fastify-cookie": "^5.6.1",
    "fastify-cors": "^6.0.3",
    "fastify-redis": "^4.3.3",
    "http": "*",
    "ioredis": "^5.0.4",
    "mongoose": "^6.3.1",
    "prettier": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rich-text": "^4.1.0",
    "sharedb": "^2.2.5",
    "sharedb-mongo": "^1.0.0",
    "url": "^0.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "nodemon": "^2.0.15"
  }
}
